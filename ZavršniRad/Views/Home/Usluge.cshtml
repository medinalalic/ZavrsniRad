
@{
    ViewBag.Title = "Usluge";
    Layout = "~/Views/Shared/_PocetnaLayout.cshtml";
}
<html>
<body>
    <!-- services -->
    <div class="services">
        <div class="container">
            <div class="services-grid">
                <h3>Šta mi nudimo</h3>
                <div class="services-grd">
                    <div class="col-md-4 services-grd1">
                        <div class="col-xs-3 services-grd1-left">
                            <span><img src="~/Content/images/left.png" width="60" height="50" /></span>
                        </div>
                        <div class="col-xs-9 services-grd1-right">
                            <h4>Ortodoncija</h4>
                            <p>
                               - Mobilni aparatić - 300 KM <br />
                               - Fiksni aparat sa običnim metalnim bravicama - 1200 KM<br />
                               - Fiksni aparat sa estetskim safirnim bravicama - 1500 KM
                            </p>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="col-md-4 services-grd1">
                        <div class="col-xs-3 services-grd1-left1">
                            <span><img src="~/Content/images/g.png" width="60" height="50" /></span>
                        </div>
                        <div class="col-xs-9 services-grd1-right">
                            <h4>Implantologija</h4>
                            <p>
                               - Vađenje zuba - 30 KM <br />
                               - Komplikovano vađenje zuba - 60 KM <br />
                               - Hirurško vađenje zuba - 100 KM <br />
                               - Vađenje zuba sa šivanjem - 70 KM
                            </p>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="col-md-4 services-grd1">
                        <div class="col-xs-3 services-grd1-left2">
                            <span><img src="~/Content/images/estetka.png" width="60" height="50" /></span>
                        </div>
                        <div class="col-xs-9 services-grd1-right">
                            <h4>Oralna hirurgija</h4>
                            <p>
                                - Uklanjanje kamenca i poliranje zuba - 40 KM <br />
                                - Odstranjivanje paradontološkog džepa - 20 KM
                            </p>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="clearfix"> </div>
                </div>
                <div class="services-grd">
                    <div class="col-md-4 services-grd1">
                        <div class="col-xs-3 services-grd1-left3">
                            <span><img src="~/Content/images/estetska.png" width="60" height="50" /></span>
                        </div>
                        <div class="col-xs-9 services-grd1-right">
                            <h4>Estetska stomatologija</h4>
                            <p>
                              - Izbjeljivanje zuba - 400 KM <br />
                              - Postavljanje cirkona - 50 KM <br />
                              
                            </p>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="col-md-4 services-grd1">
                        <div class="col-xs-3 services-grd1-left4">
                            <span><img src="~/Content/images/gggg.png" width="60" height="50" /></span>
                        </div>
                        <div class="col-xs-9 services-grd1-right">
                            <h4>Dječija i preventivna stomatologija</h4>
                            <p>
                                - Vađenje mliječnih zuba - 20 KM <br />
                                - Fluorizacija - 20 KM  <br />
                                - Uklanjanje mekih i čvrstih naslaga - 20 KM <br />
                                - Air flow - 60 KM
                            </p>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="col-md-4 services-grd1">
                        <div class="col-xs-3 services-grd1-left5">
                            <span><img src="~/Content/images/ffff.png" width="60" height="50" /></span>
                        </div>
                        <div class="col-xs-9 services-grd1-right">
                            <h4>Endodoncija</h4>
                            <p>
                               - Odstranjivanje bolesnog korijena - 30 KM <br />
                               - Stavljanje lijeka - 10 KM <br />
                               - Završno punjenje - 10 KM <br />
                               - Amputacija - 40 KM
                            </p>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="clearfix"> </div>
                </div>
            </div>
          
            <link rel="stylesheet" href=@Url.Content("/Content/css/swipebox.css")>
            <script src=@Url.Content("/Content/js/jquery.swipebox.min.js")></script>
            <script type="text/javascript">
								jQuery(function($) {
									$(".swipebox").swipebox();
								});
            </script>
        </div>
    </div>
    <!-- //services -->
</body>
</html>

